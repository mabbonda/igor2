    "bootstrap-vue": "^2.21.2",
    "cors": "^2.8.5",
    "moment": "^2.29.1",
    "v-tooltip": "^2.1.3",
    "vue": "^2.6.14",
    "vue-bootstrap4-table": "^1.1.11",
    "vue-reactive-storage": "github:ropbla9/vue-reactive-storage",
